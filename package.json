{
  "name": "explorer-base",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode prod",
    "build:dev": "vite build --mode development",
    "testnet": "bun run scripts/testnet.js",
    "test": "vitest --run",
    "test:browser": "vitest --browser.headless=false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "fix": "bun eslint . --fix && bun prettier --write \"**/*.{js,jsx,ts,tsx,json}\""
  },
  "dependencies": {
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "open-libra-sdk": "1.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native-web": "^0.20"
  },
  "devDependencies": {
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/browser": "^3.2.4",
    "docker-compose": "^1.2.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5",
    "vite": "^7",
    "vite-plugin-react-native-web": "^2",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^1",
    "webdriverio": "^9.17.0"
  }
}
