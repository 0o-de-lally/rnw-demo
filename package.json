{
  "name": "bun-react-native-web-demo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode prod",
    "build:dev": "vite build --mode development",
    "testnet": "bun run scripts/testnet.js",
    "test": "vitest --run",
    "test:browser": "vitest --browser.headless=false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "fix": "eslint . --fix && prettier --write \"**/*.{js,jsx,ts,tsx,json}\""
  },
  "dependencies": {
    "open-libra-sdk": "1.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native-web": "^0.18.12"
  },
  "devDependencies": {
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.3",
    "docker-compose": "^1.2.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.3",
    "typescript": "^4.9.5",
    "vite": "^6.3.5",
    "vite-plugin-react-native-web": "^1.2.0",
    "vitest": "^3.1.3",
    "vitest-browser-react": "^0.1.1",
    "webdriverio": "^9.12.7"
  }
}
